{
  "manifest_version": 3,
  "name": "Retab",
  "description": "당신의 브라우저 방문 기록을 한눈에 확인하세요.",
  "version": "0.1",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "permissions": ["history"],
  "action": {
    "default_popup": "index.html"
  }
}
